<!DOCTYPE html>
<html>

<head>
  {{> analytics}}
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{title}}</title>
  {{> favicon}}
  {{> css}}
  {{> cookieConsentStyles}}
</head>

<body>
  <!-- #TODO Photo Gallery -->
  {{> header}}
  <div class="container-fluid">
    <div class="row">
      <div class="col text-center">
        <h1 class="display-3 m-4 p-4">Madonna Gardens Community Photos</h1>
      </div>
    </div>
    <div class="row" id="gallery" data-toggle="modal" data-target="#lightbox">
      <div class="col-6 col-md-4 col-lg-3">
        <img class="w-100 p-2" src="img/gallery/MadonnaGardens-8822-HDR-Edit-Edit-Edit.jpg" data-target="#lightbox"
          data-slide-to="0" />
      </div>
      <div class="col-6 col-md-4 col-lg-3">
        <img class="w-100 p-2" src="img/gallery/CIA_8763.jpg" data-target="#lightbox"
          data-slide-to="1" />
      </div>
      <div class="col-6 col-md-4 col-lg-3">
        <img class="w-100 p-2" src="img/gallery/MadonnaGardens-1-9.jpg" data-target="#lightbox"
          data-slide-to="2" />
      </div>
      <div class="col-6 col-md-4 col-lg-3">
        <img class="w-100 p-2" src="img/gallery/MadonnaGardens-1-23.jpg" data-target="#lightbox"
          data-slide-to="3" />
      </div>
      <div class="col-6 col-md-4 col-lg-3">
        <img class="w-100 p-2" src="img/gallery/MadonnaGardens-1-19.jpg" data-target="#lightbox"
          data-slide-to="4" />
      </div>
      <div class="col-6 col-md-4 col-lg-3">
        <img class="w-100 p-2" src="img/gallery/DSC_2889.jpg" data-target="#lightbox"
          data-slide-to="5" />
      </div>
      <div class="col-6 col-md-4 col-lg-3">
        <img class="w-100 p-2" src="img/gallery/CIA_8549.jpg" data-target="#lightbox"
          data-slide-to="6" />
      </div>
      <div class="col-6 col-md-4 col-lg-3">
        <img class="w-100 p-2" src="img/gallery/CIA_8510.jpg" data-target="#lightbox"
          data-slide-to="7" />
      </div>
      <div class="col-6 col-md-4 col-lg-3">
        <img class="w-100 p-2" src="img/gallery/MadonnaGardens-1-15.jpg" data-target="#lightbox"
          data-slide-to="8" />
      </div>
      <div class="col-6 col-md-4 col-lg-3">
        <img class="w-100 p-2" src="img/gallery/MadonnaGardens-1-4.jpg" data-target="#lightbox"
          data-slide-to="9" />
      </div>
      <div class="col-6 col-md-4 col-lg-3">
        <img class="w-100 p-2" src="img/gallery/MadonnaGardens-8811-HDR-Edit.jpg" data-target="#lightbox"
          data-slide-to="10" />
      </div>
      <div class="col-6 col-md-4 col-lg-3">
        <img class="w-100 p-2" src="img/gallery/MadonnaGardens-1.jpg" data-target="#lightbox"
          data-slide-to="11" />
      </div>
    </div>
  </div>
  <div class="modal fade" id="lightbox" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-xl" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <p class="h5">Madonna Gardens Photos</p>
          <button type="button" class="close" data-dismiss="modal" aria-label="close"><span
              aria-hidden="true">&times;</span></button>
        </div>
        <div class="modal-body">
          <div id="carouselIndicators" class="carousel slide" data-ride="carousel">
            <ol class="carousel-indicators">
              <li data-target="#carouselIndicators" data-slide-to="0" class="active"></li>
              <li data-target="#carouselIndicators" data-slide-to="1"></li>
              <li data-target="#carouselIndicators" data-slide-to="2"></li>
              <li data-target="#carouselIndicators" data-slide-to="3"></li>
              <li data-target="#carouselIndicators" data-slide-to="4"></li>
              <li data-target="#carouselIndicators" data-slide-to="5"></li>
              <li data-target="#carouselIndicators" data-slide-to="6"></li>
              <li data-target="#carouselIndicators" data-slide-to="7"></li>
              <li data-target="#carouselIndicators" data-slide-to="8"></li>
              <li data-target="#carouselIndicators" data-slide-to="9"></li>
              <li data-target="#carouselIndicators" data-slide-to="10"></li>
              <li data-target="#carouselIndicators" data-slide-to="11"></li>
            </ol>
            <div class="carousel-inner">
              <div class="carousel-item active">
                <img src="img/gallery/MadonnaGardens-8822-HDR-Edit-Edit-Edit.jpg" class="d-block w-100" alt="...">
              </div>
              <div class="carousel-item">
                <img src="img/gallery/CIA_8763.jpg" class="d-block w-100" alt="...">
              </div>
              <div class="carousel-item">
                <img src="img/gallery/MadonnaGardens-1-9.jpg" class="d-block w-100" alt="...">
              </div>
              <div class="carousel-item">
                <img src="img/gallery/MadonnaGardens-1-23.jpg" class="d-block w-100" alt="...">
              </div>
              <div class="carousel-item">
                <img src="img/gallery/MadonnaGardens-1-19.jpg" class="d-block w-100" alt="...">
              </div>
              <div class="carousel-item">
                <img src="img/gallery/DSC_2889.jpg" class="d-block w-100" alt="...">
              </div>
              <div class="carousel-item">
                <img src="img/gallery/CIA_8549.jpg" class="d-block w-100" alt="...">
              </div>
              <div class="carousel-item">
                <img src="img/gallery/CIA_8510.jpg" class="d-block w-100" alt="...">
              </div>
              <div class="carousel-item">
                <img src="img/gallery/MadonnaGardens-8811-HDR-Edit.jpg" class="d-block w-100" alt="...">
              </div>
              <div class="carousel-item">
                <img src="img/gallery/MadonnaGardens-1-4.jpg" class="d-block w-100" alt="...">
              </div>
              <div class="carousel-item">
                <img src="img/gallery/MadonnaGardens-8811-HDR-Edit.jpg" class="d-block w-100" alt="...">
              </div>
              <div class="carousel-item">
                <img src="img/gallery/MadonnaGardens-1.jpg" class="d-block w-100" alt="...">
              </div>
            </div>
            <a class="carousel-control-prev" href="#carouselIndicators" role="button" data-slide="prev">
              <span class="carousel-control-prev-icon" aria-hidden="true"></span>
              <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carouselIndicators" role="button" data-slide="next">
              <span class="carousel-control-next-icon" aria-hidden="true"></span>
              <span class="sr-only">Next</span>
            </a>
          </div>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        </div>
      </div>
    </div>
  </div>
  {{> scripts}}
  {{> footer}}
</body>

</html>